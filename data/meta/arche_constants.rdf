            <acdh:hasCoverageEndDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1473-12-31</acdh:hasCoverageEndDate>
            <acdh:hasCreator rdf:resource="https://id.acdh.oeaw.ac.at/kgruenwald"/>
            <acdh:hasCreator rdf:resource="https://d-nb.info/gnd/143111221"/> <!-- Fiska -->
            <acdh:hasCreator rdf:resource="https://id.acdh.oeaw.ac.at/rweinbergmair"/>
